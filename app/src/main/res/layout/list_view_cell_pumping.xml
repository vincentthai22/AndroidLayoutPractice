<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/dateTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:text="This Month" />

    <TextView
        android:id="@+id/weightTotalDataTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignStart="@+id/weightTotalTextView"
        android:layout_alignTop="@+id/averageWeightDataTextView"
        android:text="0.6" />

    <TextView
        android:id="@+id/averageWeightDataTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/pumpingsDataTextView"
        android:layout_alignBottom="@+id/pumpingsDataTextView"
        android:layout_toStartOf="@+id/weightTotalTextView"
        android:text="0.3" />

    <TextView
        android:id="@+id/pumpingsDataTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/dateTypeTextView"
        android:layout_alignBottom="@+id/dateTypeTextView"
        android:layout_alignStart="@+id/pumpingsTextView"
        android:text="2.0" />

    <TextView
        android:id="@+id/weightTotalTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/averageWeightTextView"
        android:layout_alignBottom="@+id/averageWeightTextView"
        android:layout_marginStart="8dp"
        android:layout_toEndOf="@+id/averageWeightTextView"
        android:text="oz total" />

    <TextView
        android:id="@+id/averageWeightTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="17dp"
        android:layout_toEndOf="@+id/pumpingsTextView"
        android:text="oz avg" />

    <TextView
        android:id="@+id/pumpingsTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="126dp"
        android:layout_toEndOf="@+id/dateTextView"
        android:text="pumpings" />

    <TextView
        android:id="@+id/dateTypeTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/dateTextView"
        android:layout_marginTop="14dp"
        android:text="(cumulative)" />
</RelativeLayout>